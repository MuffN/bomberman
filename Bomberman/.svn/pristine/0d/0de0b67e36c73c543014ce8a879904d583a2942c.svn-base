<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.collections.*?>
<?import java.lang.String?>


<ListView id="lv_StartMenu" fx:id="lv_StartMenu" fixedCellSize="40.0"
	maxHeight="200.0" maxWidth="300.0" minHeight="200.0" minWidth="300.0"
	mouseTransparent="true" onKeyPressed="#handleKey" prefHeight="200.0"
	prefWidth="300.0" stylesheets="@menu.css" xmlns="http://javafx.com/javafx/8.0.111"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="bomberman.prototype.StartMenuController">
	<items>
		<FXCollections fx:factory="observableArrayList">
			<String fx:value="START" />
			<String fx:value="INSTRUCTIONS" />
			<String fx:value="CREDITS" />
			<String fx:value="QUIT GAME" />
		</FXCollections>
	</items>
</ListView>
